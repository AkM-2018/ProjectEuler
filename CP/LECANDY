# Given an array and number, compare the sum of array and the number

#include <iostream>
#include <vector>
using namespace std;

int main() {
	int test;
	cin>>test;
	for(int t=0;t<test;t++){
	    int n,c;
	    cin>>n>>c;
	    int arr[n];
	    int sum = 0;
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	        sum += arr[i];
	    }
	    if(sum<=c)
	        cout<<"Yes"<<endl;
	    else
	        cout<<"No"<<endl;
	}
	return 0;
}
