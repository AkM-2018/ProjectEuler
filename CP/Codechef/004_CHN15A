# Easy use of modulo operator. If divisible increase count

#include <iostream>
using namespace std;

int main() {
	int test;
	cin>>test;
	while(test--){
	    int n,k;
	    cin>>n>>k;
	    int arr[n];
	    int count = 0;
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	        if((arr[i]+k)%7 == 0)
	            count++;
	    }
	    cout<<count<<endl;
	}
	return 0;
}
